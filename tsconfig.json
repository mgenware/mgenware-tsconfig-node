{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "noUncheckedIndexedAccess": true,
    "noEmitOnError": true,
    "importHelpers": true,
    "newLine": "lf"
  }
}
